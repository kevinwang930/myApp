{
  "name": "my-app",
  "productName": "my-app",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Fast Refresh for rapid application development",
  "scripts": {
    "build": "node ./.erb/scripts/clean.js dist && concurrently npm:build:main npm:build:renderer npm:build:pythonService",
    "build:main": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.main.prod.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.renderer.prod.js",
    "build:pythonService": "conda activate electron && pyinstaller -D -y -n pythonService --distpath pythonBuild/dist --workpath pythonBuild --specpath pythonBuild src/pythonService/async_server.py  ",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "node ./.erb/scripts/clean.js build && npm run build && electron-builder build --publish never",
    "postinstall": "electron-rebuild && electron-builder install-app-deps",
    "start": "conda activate electron && node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron  ./src/main/main.js",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./.erb/configs/webpack.config.renderer.dev.js",
    "test": "jest",
    "archive": "git archive --format=zip --output=archive/latest.zip main",
    "git": "node .erb/scripts/check-git-status.js",
    "tailwind-build": "postcss src/renderer/app.global.css -o release/app/dist/index_postcss.css",
    "tailwind-build-watch": "postcss src/index.css -o dist/index_postcss.css --watch"
  },
  "build": {
    "productName": "myApp",
    "appId": "com.electron.app",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "extraFiles": [
      {
        "from": "pythonBuild/dist/pythonService",
        "to": "pythonService"
      },
      {
        "from": "db",
        "to": "db",
        "filter": [
          "!**/srcStorage${/*}"
        ]
      },
      {
        "from": "public",
        "to": "public"
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "kevinwang930",
      "repo": "electron-react-boilerplate"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinwang930/mytemplate.git"
  },
  "author": {
    "name": "Electron React Boilerplate",
    "email": "kevinwang930@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinwang930/mytemplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/kevinwang930/mytemplate#readme",
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.2",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "detect-port": "^1.3.0",
    "electron": "^13.4.0",
    "electron-builder": "^22.14.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.1",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.42.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.15",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.13",
    "@ant-design/icons": "^4.6.4",
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.5",
    "@reduxjs/toolkit": "^1.6.1",
    "antd": "^4.16.13",
    "better-sqlite3": "^7.4.3",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.6.0",
    "exceljs": "^4.3.0",
    "history": "5.x.x",
    "lodash": "^4.17.21",
    "open": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "browserslist": []
}
