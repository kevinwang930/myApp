{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron: Main",
      "type": "node",
      "request": "launch",
    //   "protocol": "inspector",
      "runtimeExecutable": "${workspaceFolder}/public/debug.cmd",
      "console":"integratedTerminal",
      "runtimeArgs": [
        "${workspaceRoot}/src/main/main.js",
        "--remote-debugging-port=9223"
      ],
      "preLaunchTask": "Start Webpack Dev"
    },
    {
      "name": "Electron: Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}",
      "timeout": 15000
    }
  ],
  "compounds": [
    {
      "name": "Electron: All",
      "configurations": ["Electron: Main", "Electron: Renderer"]
    }
  ]
}
