{
  "name": "my-app",
  "version": "1.0.6",
  "homepage": "https://github.com/kevinwang930/myApp#readme",
  "description": "electron react app for order management",
  "license": "MIT",
  "author": {
    "name": "Kevin Wang",
    "email": "kevinwang930@hotmail.com"
  },
  "main": "./release/app/dist/main/main.js",
  "scripts": {
    "archive": "git archive --format=zip --output=archive/latest.zip main",
    "build": "node ./buildConfig/scripts/prepare-build.js && concurrently npm:build:main npm:build:renderer npm:build:pythonService",
    "build:main": "webpack --config ./buildConfig/configs/webpack.config.main.prod.js",
    "build:pythonService": "conda activate electron && pyinstaller -D -y -n pythonService --distpath pythonBuild/dist --workpath pythonBuild --specpath pythonBuild src/pythonService/async_server.py  ",
    "build:renderer": "webpack --config ./buildConfig/configs/webpack.config.renderer.prod.js",
    "postinstall": "electron-builder install-app-deps",
    "package": "npm run build && electron-builder build --publish never",
    "publish": "node ./buildConfig/scripts/prepare-publish.js && npm run build && electron-builder build  --win --x64  --publish always",
    "publish:test": "node ./buildConfig/scripts/prepare-publish.js",
    "start": "conda activate electron && node ./buildConfig/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron  ./src/main/main.js",
    "start:main:debug": "cross-env NODE_ENV=development electron  ./src/main/main.js --remote-debugging-port=9222",
    "start:renderer": "webpack serve --config ./buildConfig/configs/webpack.config.renderer.dev.js",
    "test": "jest"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "better-sqlite3": "^7.4.3"
  },
  "devDependencies": {
    "@ant-design/charts": "^1.2.14",
