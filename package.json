    "@grpc/grpc-js": "^1.4.1",
    "@grpc/proto-loader": "^0.6.6",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "antd": "^4.16.13",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
